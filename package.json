{
	"name": "perl-html-template",
	"displayName": "Perl HTML Template",
	"description": "Perl HTML Template support",
	"version": "1.1.5",
	"publisher": "biamacedo",
	"icon": "./images/icon128.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/biamacedo/code-perl-html-template.git"
	},
	"galleryBanner": {
		"color": "#ededed",
		"theme": "light"
	},
	"engines": {
		"vscode": "^0.10.6"
	},
	"categories": [
		"Languages"
	],
	"contributes": {
		"languages": [{
			"id": "perl-html-template",
			"aliases": ["Perl HTML Template", "perl-html-template"],
			"extensions": [".tmpl",".inc"],
			"configuration": "./perl-html-template.configuration.json"
		}],
		"grammars": [{
			"language": "perl-html-template",
			"scopeName": "text.html.html-template",
			"path": "./syntaxes/perl-html-template.tmLanguage"
		}],
		"snippets": [{
			"language": "perl-html-template",
			"path": "./snippets/perl-html-template.json"
		}]
	}
}
