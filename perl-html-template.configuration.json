{
	"comments": {
		"lineComment": "##",
		"blockComment": [ "<TMPL_COMMENT>", "</TMPL_COMMENT>" ]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["[%", "%]"]
	],
   "autoClosingPairs": [
       { "open": "{", "close": "}" },
       { "open": "[", "close": "]" },
       { "open": "(", "close": ")" },
       { "open": "%", "close": "%" }
   ]
}
